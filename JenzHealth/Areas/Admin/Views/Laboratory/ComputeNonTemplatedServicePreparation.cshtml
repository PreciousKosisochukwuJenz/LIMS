@using JenzHealth.DAL.Entity;
@model JenzHealth.Areas.Admin.ViewModels.NonTemplatedLabPreparationVM
@{
    ViewBag.Title = "Compute Result";
}
<!-- Page Header -->
<div class="page-header row no-gutters py-4">
    <div class="col-12 col-sm-4 text-center text-sm-left mb-0">
        <span class="text-uppercase page-subtitle">Laboratory management</span>
        <h3 class="page-title">Compute Result</h3>
    </div>
</div>
@using (Html.BeginForm("Transactions", "Payment", FormMethod.Post, new { area = "Admin", @class = "validate", novalidate = true }))
{
    <div class="row">
        <div class="col">
            <div class="card  card-small h-auto">
                <div class="card-header border-bottom">
                    <h6 class="m-0">Macroscopy</h6>
                    <div class="block-handle"></div>
                </div>
                <div class="card-body">
                    <div class="m-0">
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Appearance</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Appearance, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Appearance required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Color</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Color, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Color required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Blood</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.MacrosopyBlood, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Blood required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Adult Warm</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.AdultWarm, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Adult warm required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Mucus</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Mucus, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Mucus required
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <hr />
            <div class="card  card-small h-auto">
                <div class="card-header border-bottom">
                    <h6 class="m-0">Dipstick</h6>
                    <div class="block-handle"></div>
                </div>
                <div class="card-body">
                    <div class="m-0">
                        <div class="row">
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Specific Gravity</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.SpecificGravity, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Specific gravity required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Blirubin</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Blirubin, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Blirubin required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Acidity (PH)</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Acidity, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Acidity required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Urobilinogen</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Urobilinogen, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Urobilinogen required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Glucose</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Glucose, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Glucose required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Ascorbic Acid</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.AscorbicAcid, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Ascorbic acid required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Protein</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Protein, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Protein required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Blood</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.DipstickBlood, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Blood required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Niterite</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Niterite, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Niterite required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Leucocyte Esterase</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.LeucocyteEsterase, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Leucocyte esterase required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Ketones</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Ketones, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Ketones required
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <hr />

            <div class="card  card-small h-auto">
                <div class="card-header border-bottom">
                    <h6 class="m-0">Mircoscopy</h6>
                    <div class="block-handle"></div>
                </div>
                <div class="card-body">
                </div>
            </div>
            <hr />

            <div class="card  card-small h-auto">
                <div class="card-header border-bottom">
                    <h6 class="m-0">Culture</h6>
                    <div class="block-handle"></div>
                </div>
                <div class="card-body">
                    <div class="m-0">
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Temperature</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Temperature, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Temperature required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Duration</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Duration, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Duration required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Atomsphere</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Atomsphere, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Atomsphere required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Plate</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Plate, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Plate required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label for="socialGitHub">Incubatio</label>
                                <div class="input-group input-group-seamless">
                                    @Html.EditorFor(model => model.Incubatio, new { htmlAttributes = new { @class = "form-control", placeholder = "", required = true } })
                                    <div class="invalid-feedback">
                                        Incubatio required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">Line Organism</label>
                                <div class="input-group input-group-seamless">
                                    <input type="text" class="form-control" id="organismField" />
                                    <div class="invalid-feedback">
                                        Organims required
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="socialGitHub">-</label>
                                <div class="input-group">
                                    <button type="button" id="AddOrganism" class="btn btn-outline-primary">Add</button>
                                </div>
                            </div>
                            <div class="card-body p-0 pb-3 text-center">
                                <div id="OrganismTableLoader" style="display:none" class="col text-center">
                                    <img src="~/Content/images/Spinner.gif" style="width:50px;height:50px" />
                                </div>
                                <div id="OrganismTableDiv">
                                    <table class="file-manager file-manager-list2 d-none table-responsive">
                                        <thead class="bg-light">
                                            <tr>
                                                <th scope="col" class="border-0"></th>
                                                <th scope="col" class="border-0">Organism</th>
                                                <th scope="col" class="border-0">Anti Biotics</th>
                                            </tr>
                                        </thead>
                                        <tbody id="OrganismBody">
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <hr />
                <div class="card-footer">
                    <div class="row">
                        <div class="form-group col-md-12">
                            <label for="labnote">Lab Note</label>
                            <div class="input-group input-group-seamless">
                                @Html.TextAreaFor(model => model.Labnote, 5, 0, new { @class = "form-control", placeholder = "", required = true })
                                <div class="invalid-feedback">
                                    Lab note required
                                </div>
                            </div>
                        </div>
                        <input type="hidden" id="billnumber" name="BillInvoiceNumber" value="@TempData["BillNumber"]" />
                        <input type="hidden" id="SpecimenCollectedID" name="SpecimenCollectedID" value="@TempData["SpecimenCollectedID"]" />
                        <div class="col-md-12 text-center">
                            <button class="btn btn-primary" id="FinishBtn" type="button">
                                Save
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<br />
@section Scripts{
    <script src="~/Areas/Admin/Scripts/ComputeNonTemplated.js"></script>
}


